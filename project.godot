config_version=5

[application]

run/main_scene="res://WORLD.tscn"
config/features=PackedStringArray("4.2")

[autoload]

PlayerStats="*res://scripts/singletons/PlayerStats.gd"
Hotkeys="*res://scripts/singletons/hotkeys.gd"
Util="*res://scripts/singletons/utilities.gd"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
window/vsync/vsync_mode=2

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/beehave/plugin.cfg", "res://addons/kanban_tasks/plugin.cfg")

